<!--pages/manager/manager.wxml-->
<view class="container">
  <image src='{{code}}' class='code' bindtap='imgYu'></image>
  <text class='item'>{{communityName}}</text>
  <text class='id' bindtap='copyText'>ID：{{id}}</text>
  <view class='data' bindtap="JumpToStatistics">查看数据</view>
  <view class='data' bindtap="ExportData">导出数据</view>
  <view class='exit' bindtap="ReturnComRegister">退出社区</view>
</view>
